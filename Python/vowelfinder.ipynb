   "source": [
    "define a function which genrates a string comprises of consecutive vowels of the given string."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "def solve(A):\n",
    "    vowels = ['a','e','i','o','u']\n",
    "    s = \"\"\n",
    "    for a in A :\n",
    "        if a in vowels :\n",
    "            s += a\n",
    "    return s"
   ]
  },
